library(philentropy)
data <- c(15,0,5,0,20,0,6,3,5,2,7,0,1,0,3,0,1,1,8,9,3,5,1,7)
data <- matrix(data, byrow =T, nrow=8, ncol=3)
# L1 norm / Manhattan distance
distance(data, method="manhattan")
# L2 norm / Euclidean distance
distance(data, method="euclidean")
# L∞ (supremum) / Chebyshev
distance(data, method="chebyshev")
# Cosine
distance(data, method="cosine")
